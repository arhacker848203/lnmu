{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://lnmuv2-production.up.railway.app/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "https://lnmuniversity.com/:path*"
    },
    {
      "source": "/qrcode/:path*",
      "destination": "https://api.qrserver.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
